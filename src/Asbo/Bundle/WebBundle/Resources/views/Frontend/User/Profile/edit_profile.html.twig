{% extends "AsboWebBundle:Frontend/User/Profile:layout.html.twig" %}

{% block title %}Editer son profil - {{ parent() }}{% endblock %}
{% block page_title %} Modifier son profil <br /> <small>Ici, je modifie ce que je veux !</small>{% endblock %}

{% block profile_content %}
<div class="span9">
    <form action="{{ path('sonata_user_profile_edit') }}" method="POST">
        {{ form_rest(form) }}

        <input type="submit" name="submit" class="btn" value="{{ 'sonata_user_submit'|trans({}, 'SonataUserBundle') }}" />
    </form>
</div>
{% endblock %}
