default:
    formatter:
        name: pretty
    context:
        class: Asbo\Bundle\WebBundle\Behat\FeatureContext
    extensions:
        Behat\MinkExtension\Extension:
            default_session: symfony2
            goutte: ~
            browser_name: firefox
        Behat\Symfony2Extension\Extension:
            mink_driver: true